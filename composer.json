{
  "name": "awjudd/guard-clauses",
  "description": "A simple package with guard clause helpers.",
  "type": "library",
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "phpstan/phpstan": "^1.11"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "JuddDev\\GuardClauses\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Andrew Judd",
      "email": "andrew@judd.dev"
    }
  ],
  "minimum-stability": "stable",
  "scripts": {
    "phpunit": "vendor/bin/phpunit tests",
    "phpstan": "vendor/bin/phpstan analyse -l 8 src tests"
  }
}
